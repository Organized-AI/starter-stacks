{
  "name": "@organized-ai/stack-evaluator",
  "version": "1.0.0",
  "description": "AI Tool Stack Evaluator - Professional CLI for developers to find the perfect tech stack in 2 minutes",
  "main": "dist/index.js",
  "bin": {
    "stack-eval": "dist/bin/cli.js",
    "stack-evaluator": "dist/bin/cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx src/bin/cli.ts",
    "start": "node dist/bin/cli.js",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run build",
    "local-install": "npm run build && npm link",
    "local-uninstall": "npm unlink -g @organized-ai/stack-evaluator"
  },
  "keywords": [
    "ai",
    "cli",
    "stack",
    "evaluator",
    "tech-stack",
    "developer-tools",
    "ai-tools",
    "recommendation",
    "scaffold",
    "generator",
    "nextjs",
    "fastapi",
    "typescript"
  ],
  "author": {
    "name": "Organized AI",
    "email": "hello@organized.ai",
    "url": "https://organized.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Organized-AI/starter-stacks.git",
    "directory": "tools/stack-evaluator/cli"
  },
  "bugs": {
    "url": "https://github.com/Organized-AI/starter-stacks/issues"
  },
  "homepage": "https://stack.organizedai.vip",
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "ora": "^5.4.1",
    "semver": "^7.5.4",
    "simple-git": "^3.20.0",
    "update-notifier": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.5",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.11.5",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
